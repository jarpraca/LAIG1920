        <!-- SCENE -->
        <component id="scene">
            <transformation>
                <translate x="0" y="0" z="0" />
            </transformation>
            <materials>
                <material id="plane_white" />
            </materials>
            <texture id="none"/>
            <children>
                <componentref id="plane" />
                <componentref id="ground" />
            </children>
        </component>
    </components>
